{
  "name": "picturama",
  "productName": "Picturama",
  "version": "1.2.0-dev",
  "description": "Digital image organizer powered by the web",
  "author": "Lo√Øc Nogues <nogues.loic@gmail.com>, Til Schneider <github@murfman.de>",
  "license": "MIT",
  "main": "dist/background.js",
  "engineStrict": true,
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "build": {
    "appId": "github.picturama",
    "afterSign": "src/package/afterSignHook.js",
    "mac": {
      "category": "public.app-category.photography",
      "hardenedRuntime": true,
      "entitlements": "src/package/entitlements.mac.plist",
      "electronLanguages": [
        "de",
        "en",
        "es",
        "bg"
      ]
    },
    "linux": {
      "category": "Graphics",
      "asar": false
    },
    "publish": null,
    "directories": {
      "output": "dist-package",
      "buildResources": "src/package"
    },
    "files": [
      "dist/**",
      "!**/node_modules/sharp/docs",
      "!**/node_modules/sharp/src",
      "!**/node_modules/sqlite3/src"
    ],
    "extraResources": {
      "to": "app",
      "filter": [
        "menus/**",
        "migrations/**"
      ]
    }
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "clean": "rm -rf dist dist-package",
    "clean-package": "rm -rf dist-package",
    "start": "yarn run build && yarn run start-no-build",
    "watch": "NODE_ENV=development webpack -w",
    "build": "NODE_ENV=development webpack",
    "build-production": "NODE_ENV=production webpack",
    "start-no-build": "PICTURAMA_DEV_MODE=true electron .",
    "package-dir": "electron-builder --dir",
    "package": "electron-builder",
    "release": "yarn run clean && yarn run build-production && yarn run package",
    "release:windows": "yarn run clean && set \"NODE_ENV=production\" && webpack && yarn run package",
    "test": "jest",
    "test:clean": "rm -rf dist-test"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "runner": "@jest-runner/electron/main",
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.test)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "((app|background|common|test-jest)/.*)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "@blueprintjs/core": "^3.22.3",
    "@blueprintjs/select": "^3.11.2",
    "bluebird": "^3.7.2",
    "chokidar": "3.3.1",
    "classnames": "^2.2.6",
    "copy-text-to-clipboard": "^2.1.1",
    "electron": "7.1.7",
    "exif-parser": "^0.1.12",
    "font-awesome": "^4.7.0",
    "fs.extra": "^1.3.2",
    "gl-matrix": "^3.1.0",
    "image-size": "^0.8.3",
    "js-yaml": "^3.13.1",
    "justified-layout": "^3.0.0",
    "moment": "^2.24.0",
    "node-notifier": "6.0.0",
    "node-pre-gyp": "^0.14.0",
    "normalize.css": "^8.0.1",
    "piexifjs": "1.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "^2.3.0",
    "sharp": "0.23.4",
    "source-map-support": "^0.5.16",
    "sourcemapped-stacktrace": "^1.1.11",
    "sqlite3": "^4.1.1",
    "sqlite3-helper": "1.3.2",
    "typesafe-actions": "^5.1.0"
  },
  "optionalDependencies": {
    "libraw": "file:submodules/node-libraw"
  },
  "devDependencies": {
    "@jest-runner/electron": "^2.0.3",
    "@types/bluebird": "^3.5.29",
    "@types/classnames": "^2.2.9",
    "@types/gl-matrix": "^2.4.5",
    "@types/jest": "^24.0.25",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "~12.0.12",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/sharp": "^0.23.1",
    "@types/sqlite3": "^3.1.6",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.1",
    "electron-builder": "^21.2.0",
    "electron-notarize": "^0.2.1",
    "file-loader": "^5.0.2",
    "jest": "^24.9.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-gyp": "^5.0.7",
    "style-loader": "^1.1.2",
    "ts-jest": "^24.2.0",
    "typescript": "~3.7.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-fix-default-import-plugin": "^1.0.3",
    "webpack-node-externals": "^1.7.2"
  },
  "repository": "https://github.com/picturama/picturama.git"
}
