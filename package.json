{
  "name": "picturama",
  "productName": "Picturama",
  "version": "1.4.0-dev",
  "description": "Digital image organizer powered by the web",
  "author": "Lo√Øc Nogues <nogues.loic@gmail.com>, Til Schneider <github@murfman.de>",
  "license": "MIT",
  "main": "dist/background.js",
  "engineStrict": true,
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "build": {
    "appId": "github.picturama",
    "afterSign": "src/package/afterSignHook.js",
    "mac": {
      "category": "public.app-category.photography",
      "hardenedRuntime": true,
      "entitlements": "src/package/entitlements.mac.plist",
      "electronLanguages": [
        "cs",
        "de",
        "en",
        "es",
        "fr"
      ]
    },
    "linux": {
      "category": "Graphics"
    },
    "publish": null,
    "directories": {
      "output": "dist-package",
      "buildResources": "src/package"
    },
    "files": [
      "dist/**",
      "!**/node_modules/sharp/docs",
      "!**/node_modules/sharp/src",
      "!**/node_modules/sqlite3/src"
    ],
    "extraResources": {
      "to": "app",
      "filter": [
        "migrations/**"
      ]
    }
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "clean": "rm -rf dist dist-package",
    "clean-package": "rm -rf dist-package",
    "start": "yarn run build && yarn run start-no-build",
    "watch": "npm run env NODE_ENV=development -- webpack -w",
    "build": "npm run env NODE_ENV=development -- webpack",
    "build-production": "npm run env NODE_ENV=production -- webpack",
    "i18n": "node src/script/check-i18n.mjs",
    "start-no-build": "npm run env PICTURAMA_DEV_MODE=true -- electron .",
    "package-dir": "electron-builder --dir",
    "package": "electron-builder",
    "release": "yarn run clean && yarn run i18n && yarn run build-production && yarn run package",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:clean": "rm -rf dist-test"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "runner": "@jest-runner/electron/main",
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.test)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "((app|background|common|test-jest)/.*)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "@blueprintjs/core": "^3.28.2",
    "@blueprintjs/select": "^3.13.3",
    "bluebird": "^3.7.2",
    "chokidar": "3.4.0",
    "classnames": "^2.2.6",
    "copy-text-to-clipboard": "^2.2.0",
    "exifr": "^5.0.3",
    "font-awesome": "^4.7.0",
    "gl-matrix": "^3.1.0",
    "heic2any": "^0.0.3",
    "image-size": "^0.8.3",
    "js-yaml": "^3.14.0",
    "justified-layout": "^4.0.0",
    "moment": "^2.26.0",
    "node-notifier": "7.0.1",
    "node-pre-gyp": "^0.15.0",
    "normalize.css": "^8.0.1",
    "piexifjs": "1.0.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "^2.3.0",
    "source-map-support": "^0.5.19",
    "sourcemapped-stacktrace": "^1.1.11",
    "sqlite3": "^4.2.0",
    "sqlite3-helper": "1.3.3",
    "typesafe-actions": "^5.1.0"
  },
  "optionalDependencies": {
    "libraw": "file:submodules/node-libraw"
  },
  "devDependencies": {
    "@jest-runner/electron": "^3.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/classnames": "^2.2.10",
    "@types/fs-extra": "~8.0.1",
    "@types/jest": "^26.0.10",
    "@types/js-yaml": "^3.12.4",
    "@types/node": "~12.0.0",
    "@types/react": "^16.9.37",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/sqlite3": "^3.1.6",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "electron": "~9.0.4",
    "electron-builder": "^22.7.0",
    "electron-notarize": "^1.0.0",
    "file-loader": "^6.0.0",
    "jest": "^26.4.2",
    "less": "^3.11.3",
    "less-loader": "^6.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-gyp": "^7.0.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.3.0",
    "typescript": "~3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-fix-default-import-plugin": "^1.0.3",
    "webpack-node-externals": "^1.7.2"
  },
  "repository": "https://github.com/picturama/picturama.git"
}
